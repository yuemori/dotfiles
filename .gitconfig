[user]
	name = yuemori
	email = yuemori@aiming-inc.com
[color]
	ui = true
[alias]
	ch = checkout
	st = status
	co = commit
	ad = add -u
	br = branch
	tree = log --graph --decorate --oneline -n 10
	lp = log -p
	ls = log --stat
  log-all = log --graph --all --color --pretty=format:'%x09%h %cn%x09%s %Cred%d%Creset'
  log-graph = log --graph --date=short --pretty=format:'%Cgreen%h %cd %Cblue%cn %Creset%s'
  current-branch = "!sh -c \"git branch | grep '*' | awk '{ print \\$2 }' \""
  push-for-review = "!sh -c 'git push origin HEAD:refs/for/master/`git current-branch`'"
	ds = diff --staged
	dc = diff --cached
	cp = cherry-pick
	dh = diff HEAD
	amend = commit --amend
	s = status
	a = add
	am = commit --amend
	c = commit
	b = branch
	t = log --graph --decorate --oneline -n 10
	l = log
	r = rebase
	rc = rebase --continue
	rs = rebase --skip
	mt = mergetool

[credential]
	helper = osxkeychain
[core]
	editor = vim
	excludesfile = /Users/yuemori/.gitignore_global
	filemode = true
[merge]
	tool = vimdiff
[ghq]
	root = /Users/yuemori/ghq
[mergetool]
	keepBackup = false
[push]
	default = upstream
[rebase]
	autosquash = true
[init]
	templatedir = ~/.git_tmp
